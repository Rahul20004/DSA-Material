class Solution {
    // Function to sort the array using bubble sort algorithm.
    public static void bubbleSort(int arr[]) {
        int n = arr.length;
        int temp ;
        for(int i = 0; i < n-1; i++){
        
            for(int j = 1; j<n-i; j++){
                if(arr[j-1] > arr[j]){
                    temp = arr[j-1];
                    arr[j-1] = arr[j]; 
                    arr[j] = temp;
                    
                }
            }
            
        }
        
        
        
        
    }
}
